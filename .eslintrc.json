{
  "extends": ["eslint:recommended", "google"],
  "env": {
    "browser": true
  },
  "plugins": [
    "html"
  ],
  "rules": {
    "no-var": "off",
    "new-cap": ["error", { "capIsNewExceptions": ["Polymer"] }],
    "max-len": ["error", 120],
    "no-console": "off"
  },
  "globals": {
    "Polymer": true
  }
}